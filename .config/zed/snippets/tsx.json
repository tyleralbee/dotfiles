{
    "Storybook story": {
        "prefix": "story",
        "body": [
            "import { Meta, StoryObj } from \"@storybook/react-vite\";",
            "",
            "import { ${1} } from \"./${1}\";",
            "",
            "const meta: Meta<typeof ${1}> = {",
            "    component: ${1},",
            "};",
            "export default meta;",
            "",
            "type Story = StoryObj<typeof ${1}>;",
            "",
            "export const Primary: Story = {",
            "    args: {$0},",
            "};"
        ],
        "description": "Creates a Storybook story for the ${1} component."
    },
    "React component": {
        "prefix": "tsx",
        "body": [
            "import styles from \"./${1}.module.css\";",
            "",
            "import * as React from \"react\";",
            "",
            "type ${1}Props = {",
            "    ${0}",
            "};",
            "",
            "export function ${1}({ }: ${1}Props) {",
            "    return (",
            "        <div className={styles.container}>",
            "        </div>",
            "    );",
            "}"
        ],
        "description": "Creates a React component with a CSS module."
    }
}
