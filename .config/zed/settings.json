{
    "active_pane_modifiers": { "inactive_opacity": 0.75 },
    "agent": {
        "default_model": { "provider": "copilot_chat", "model": "o3-mini" },
        "version": "2",
        "default_width": 640
    },
    "autosave": { "after_delay": { "milliseconds": 1000 } },
    "base_keymap": "VSCode",
    "buffer_font_family": "JetBrainsMono Nerd Font",
    "buffer_font_size": 12,
    "collaboration_panel": { "button": false },
    "edit_predictions": {
        "mode": "eager",
        "copilot": { "proxy": null, "proxy_no_verify": null },
        "enabled_in_text_threads": false
    },
    "features": { "edit_prediction_provider": "copilot" },
    "file_types": {
        "env": [".env*"],
        "JSON": [".prettierrc"],
        "JSONC": [
            "**/.vscode/**/*.json",
            "**/packages/tsconfig/*.json",
            "**/turbo.json",
            "**/host.json",
            "**/local.settings.json",
            "**/tsconfig.json"
        ],
        "LOG": ["*.log", "*.log.*"]
    },
    "gutter": { "code_actions": false },
    "formatter": "prettier",
    "git": { "inline_blame": { "show_commit_summary": true } },
    "icon_theme": "Zed (Default)",
    "notification_panel": {
        "button": false
    },
    "preview_tabs": {
        "enable_preview_from_file_finder": false,
        "enable_preview_from_code_navigation": true
    },
    "project_panel": {
        "show_diagnostics": "all",
        "auto_fold_dirs": false,
        "default_width": 400,
        "entry_spacing": "standard",
        "indent_guides": { "show": "always" },
        "indent_size": 12
    },
    "relative_line_numbers": true,
    "scroll_sensitivity": 3,
    "search": { "case_sensitive": true, "regex": false, "whole_word": false },
    "show_edit_predictions": true,
    "soft_wrap": "none",
    "tab_bar": {
        "show_nav_history_buttons": false,
        "show_tab_bar_buttons": false
    },
    "tab_size": 4,
    "tabs": { "git_status": true, "show_diagnostics": "all" },
    "telemetry": { "diagnostics": true, "metrics": false },
    "terminal": {
        "button": false,
        "font_family": "JetBrainsMono Nerd Font",
        "font_size": 12
    },
    "theme": { "mode": "system", "light": "One Light", "dark": "Ayu Dark" },
    "toolbar": {
        "quick_actions": false,
        "selections_menu": false,
        "breadcrumbs": false
    },
    "ui_font_family": "JetBrainsMono Nerd Font",
    "ui_font_size": 12,
    "use_autoclose": false,
    "use_smartcase_search": true,
    "vim": {
        "use_multiline_find": true,
        "use_smartcase_find": true,
        "toggle_relative_line_numbers": true,
        "highlight_on_yank_duration": 100,
        "use_system_clipboard": "always"
    },
    "git_panel": { "dock": "right", "default_width": 640 },
    "vim_mode": true
}
